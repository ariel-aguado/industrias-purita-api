{
  "kind": "collectionType",
  "collectionName": "novedades",
  "info": {
    "name": "Novedades"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "richtext",
      "required": true
    },
    "icono": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "productos": {
      "collection": "productos",
      "via": "novedade"
    },
    "posicion": {
      "type": "integer"
    }
  }
}
